{
  "name": "user-auth-template-restapi",
  "version": "1.0.0",
  "description": "User authentication template using React, React-redux, Redux-saga, Node.js, Express, MongoDB, Mongoose, JWT, Material UI",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oleksii-kyrsenko/user-auth-template-restapi.git"
  },
  "keywords": [
    "React",
    "React-redux",
    "Redux-saga",
    "Node.js",
    "Express",
    "MongoDB",
    "Mongoose",
    "JWT",
    "Material UI"
  ],
  "author": "Oleksii Kyrsenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oleksii-kyrsenko/user-auth-template-restapi/issues"
  },
  "homepage": "https://github.com/oleksii-kyrsenko/user-auth-template-restapi#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
